&lt;object id=&quot;vp1K6VtX&quot; width=&quot;540&quot; height=&quot;300&quot; classid=&quot;clsid:d27cdb6e-ae6d-11cf-96b8-444553540000&quot;&gt;&lt;param name=&quot;movie&quot; value=&quot;http://static.animoto.com/swf/w.swf?w=swf/vp1&amp;e=1331065981&amp;f=K6VtX5NaY1A0sUgwAguU0g&amp;d=277&amp;m=a&amp;r=360p&amp;volume=100&amp;start_res=360p&amp;i=m&amp;options=&quot;&gt;&lt;/param&gt;&lt;param name=&quot;allowFullScreen&quot; value=&quot;true&quot;&gt;&lt;/param&gt;&lt;param name=&quot;allowscriptaccess&quot; value=&quot;always&quot;&gt;&lt;/param&gt;&lt;embed id=&quot;vp1K6VtX&quot; src=&quot;http://static.animoto.com/swf/w.swf?w=swf/vp1&amp;e=1331065981&amp;f=K6VtX5NaY1A0sUgwAguU0g&amp;d=277&amp;m=a&amp;r=360p&amp;volume=100&amp;start_res=360p&amp;i=m&amp;options=&quot; type=&quot;application/x-shockwave-flash&quot; allowscriptaccess=&quot;always&quot; allowfullscreen=&quot;true&quot; width=&quot;540&quot; height=&quot;300&quot;&gt;&lt;/embed&gt;&lt;/object&gt;&lt;/div&gt;
